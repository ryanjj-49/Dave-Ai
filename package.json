    "install:panel": "npm install --legacy-peer-deps",
    "install:force": "npm install --force",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t docker run -e SESSION_ID=$SESSION_ID Dave-md"
